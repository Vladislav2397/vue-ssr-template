        "babel-loader": "^7.1.2",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-preset-env": "^1.6.1",
        "chokidar": "^1.7.0",
        "css-loader": "^0.28.7",
        "file-loader": "^1.1.5",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "rimraf": "^2.6.2",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.1",
        "sw-precache-webpack-plugin": "^0.11.4",
        "ts-loader": "^8.4.0",
        "url-loader": "^0.6.2",
        "vue-loader": "^15.3.0",
        "webpack": "^3.8.1",
        "webpack-dev-middleware": "^1.12.0",
